{
  "name": "web-admin-template",
  "title": "卡次元管理系统",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "prepare": "npx husky install",
    "preinstall": "npx only-allow pnpm -y",
    "start:dev": "vite --mode development",
    "start:test": "vite --mode test",
    "start:preview": "vite --mode preview",
    "start:production": "vite --mode production",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode development",
    "build:production": "vite build --mode production",
    "build:preview": "vite build --mode preview",
    "preview": "vite preview",
    "lint": "npx lint-staged",
    "profile": "vite --profile --open",
    "eslint:fix": "npx eslint --fix",
    "stylelint:fix": "npx stylelint src/*.{html,vue,css,sass,scss} --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@am/oa": "^1.7.0",
    "@unocss/reset": "^0.58.9",
    "ant-design-vue": "^4.2.5",
    "axios": "^1.7.7",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "oidc-client": "^1.11.5",
    "pinia": "^2.2.2",
    "vue": "^3.5.8",
    "vue-plugin-hiprint": "^0.0.60",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@unocss/preset-wind": "^0.58.9",
    "@unocss/transformer-compile-class": "^0.58.9",
    "@unocss/transformer-directives": "^0.58.9",
    "@unocss/transformer-variant-group": "^0.58.9",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/coverage-istanbul": "^2.1.1",
    "@vue/test-utils": "^2.4.6",
    "commitlint": "^17.8.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^8.0.3",
    "jsdom": "^25.0.1",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "sass": "^1.79.3",
    "stylelint": "^13.13.1",
    "stylelint-config-mixup": "^2.3.14",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-scss": "^4.7.0",
    "terser": "^5.43.1",
    "typescript": "^5.6.2",
    "unocss": "^0.58.9",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.4.7",
    "vitest": "^2.1.1",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run eslint:fix",
      "git add ."
    ],
    "*.html,vue,css,sass,scss": [
      "npm run stylelint:fix",
      "git add ."
    ]
  }
}